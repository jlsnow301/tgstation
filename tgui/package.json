{
	"private": true,
	"name": "tgui-workspace",
	"version": "6.0.0",
	"packageManager": "bun@1.2.16",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"tgui:analyze": "webpack --analyze",
		"tgui:bench": "webpack --env TGUI_BENCH=1 && bun packages/tgui-bench/index.js",
		"tgui:build": "webpack",
		"tgui:dev": "bun packages/tgui-dev-server/index.js",
		"tgui:fix": "biome check --write .",
		"tgui:lint": "biome check .",
		"tgui:test": "bun test",
		"tgui:tsc": "tsc"
	},
	"dependencies": {
		"@swc/core": "^1.12.1",
		"css-loader": "^7.1.2",
		"esbuild-loader": "^4.3.0",
		"mini-css-extract-plugin": "^2.9.2",
		"sass": "^1.80.6",
		"sass-loader": "^16.0.3",
		"swc-loader": "^0.2.6",
		"typescript": "^5.8.3",
		"webpack": "^5.99.9",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^6.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0-beta.6",
		"@happy-dom/global-registrator": "^17.4.7",
		"@types/bun": "^1.2.14",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"@types/webpack-env": "^1.18.8",
		"@types/wicg-file-system-access": "^2023.10.6"
	}
}
